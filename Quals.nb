Cell[BoxData[{
 RowBox[{
  RowBox[{"\[Psi]p", "=", 
   RowBox[{
    FractionBox["1", 
     SqrtBox["2"]], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["\[Psi]", "a"], "[", 
        SubscriptBox["r", "1"], "]"}], 
       RowBox[{
        SubscriptBox["\[Psi]", "b"], "[", 
        SubscriptBox["r", "2"], "]"}]}], "+", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[Psi]", "b"], "[", 
        SubscriptBox["r", "1"], "]"}], 
       RowBox[{
        SubscriptBox["\[Psi]", "a"], "[", 
        SubscriptBox["r", "2"], "]"}]}]}], ")"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Psi]m", "=", 
   RowBox[{
    FractionBox["1", 
     SqrtBox["2"]], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["\[Psi]", "a"], "[", 
        SubscriptBox["r", "1"], "]"}], 
       RowBox[{
        SubscriptBox["\[Psi]", "b"], "[", 
        SubscriptBox["r", "2"], "]"}]}], "-", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[Psi]", "b"], "[", 
        SubscriptBox["r", "1"], "]"}], 
